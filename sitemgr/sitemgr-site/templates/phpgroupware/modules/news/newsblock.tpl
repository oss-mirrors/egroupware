<!-- BEGIN RssBlock -->
<div align="right"><a href="{rsslink}">XML RSS</a></div>
<!-- END RssBlock -->
<!-- BEGIN NewsBlock -->
<div align="right">
<table width="95%" border="0" cellspacing="0" cellpadding="0">
<tr>
	<td bgcolor="537991" height="23" width="432">
		<b>
			<font color="#FFFFFF" size="2" face="Verdana, Arial, Helvetica, sans-serif">
				{news_title}
			</font>
		</b>
	</td>
</tr>
<tr> 
	<td height="23"> 
		<table width="100%" border="0" cellspacing="0" cellpadding="0">
			<tr> 
				<td> 
					<p>
					<font size="1" color="#FFFFFF" face="Verdana, Arial, Helvetica, sans-serif">
						<i>Submitted by '{news_submitter}' {news_date}</i>
						<br>
						<br>
						<font size="2">
							{news_content}
						</font>
					</font>
					</p>
				</td>
			</tr>
		</table>
	</td>
</tr>
</table>
</div>
<br><br>
<!-- END NewsBlock -->
