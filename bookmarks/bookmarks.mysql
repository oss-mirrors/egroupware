create table bookmarks (
	con 		int NOT NULL auto_increment,
	owner		varchar(255),
	category	int NOT NULL,
	url		varchar(255),
	title		varchar(255),
	des		text,
	access		enum('public','private'),
	lastupdate	int,
	lastview	int,
	totalviews	int,
	primary key(con)
);

create table bookmarks_cats (
	con 		int NOT NULL auto_increment,
	owner		varchar(255),
	parent		int,
	parent_name	varchar(255),
	type		enum('main','sub'),
	name		varchar(255),
	primary key(con)
);

