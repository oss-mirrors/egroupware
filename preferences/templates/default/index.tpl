<!-- BEGIN list -->
<table border="0" width="75%" cellspacing="0" cellpadding="0">
 <tr>
  <td align="left">{tabs}</td>
 </tr>
</table>

<table width="75%" border="0" cellspacing="0" cellpadding="0">
 {rows}
</table>
<!-- END list -->

<!-- BEGIN app_row -->
 <tr class="th_bright">
  <td width="5%" valign="middle"><img src="{app_icon}" alt="[ {app_title} ]"> <a name="{app_name}"></a></td>
  <td width="95%" valign="middle"><b>&nbsp;&nbsp;{app_title}</b></td>
 </tr>
<!-- END app_row -->

<!-- BEGIN app_row_noicon -->
 <tr class="th_bright">
  <td height="25" colspan="2" width="95%" valign="middle"><b>&nbsp;{app_title}</b> <a name="{app_name}"></a></td>
 </tr>
<!-- END app_row_noicon -->

<!-- BEGIN link_row -->
 <tr>
  <td colspan="2">&nbsp;&#8226;&nbsp;<a href="{pref_link}">{pref_text}</a></td>
 </tr>
<!-- END link_row -->

<!-- BEGIN spacer_row -->
 <tr>
  <td colspan="2">&nbsp;</td>
 </tr>
<!-- END spacer_row -->
