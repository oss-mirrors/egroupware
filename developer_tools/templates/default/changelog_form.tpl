<!-- BEGIN form -->
<p><center>{messages}</center>
<form action="{form_action}" method="POST">
 <table border="0" width="60%" align="center">
  <tr>
   <td colspan="2" bgcolor="{th_bg}">&nbsp;</td>
  </tr>

  <tr class="row_off">
   <td>{lang_project}</td>
   <td>{value_project}&nbsp;</td>
  </tr>

  <tr class="row_on">
   <td>{lang_version}</td>
   <td>{value_version}&nbsp;</td>
  </tr>

  <tr class="row_off">
   <td>{lang_change}</td>
   <td>{value_change}&nbsp;</td>
  </tr>

  <tr>
   <td align="left">{button_cancel}</td>
   <td align="right">{button_submit}</td>
  </tr>
 </table>
</form>
<!-- END form -->
