<!-- $Id$ -->
{printer_friendly}
<table id="calendar_minical_table" border="0" width="100%" cols="5">
	<tr>
		<td align="left" valign="top" width="20%">
			{small_calendar_prev}
		</td>
		<td align="center" valign="middle" width="20%">
			<b>{prev_week_link}</b>
		</td>
		<td align="center" valign="top" width="20%">
			{small_calendar_this}
		</td>
		<td align="center" valign="middle" width="20%">
			<b>{next_week_link}</b>
		</td>
		<td align="right" valign="top" width="20%">
			{small_calendar_next}
		</td>
	</tr>
</table>

<table id="calendar_week_identifier_table" class="calendar_week_identifier_table">
	<tr>
		<td align="center">
			<span class="calendar_week_identifier">{week_identifier}&nbsp;&nbsp;</span>
			<span class="calendar_user_identifier">[&nbsp;{username}&nbsp;]</span>
		</td>
	</tr>
</table>
{week_display}
<div class="calendar_link_print">
{print}
</div>